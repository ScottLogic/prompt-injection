{
	"name": "prompt-injection-ui",
	"version": "1.1.0",
	"type": "module",
	"scripts": {
		"dev": "vite --open",
		"build": "tsc && vite build",
		"preview": "vite preview --strictPort --port 5173 --open",
		"codecheck": "concurrently \"prettier --check .\" \"eslint --max-warnings 0 .\" \"stylelint **/*.css --mw 0\"",
		"format": "prettier --write .",
		"lint": "concurrently \"eslint . --fix\" \"stylelint **/*.css --fix\"",
		"pretest": "tsc --project tsconfig.test.json",
		"test": "vitest"
	},
	"dependencies": {
		"@aws-amplify/ui-react": "^6.1.7",
		"@cyntler/react-doc-viewer": "^1.17.0",
		"@emotion/react": "^11.13.3",
		"@emotion/styled": "^11.13.0",
		"@mui/material": "^5.16.7",
		"@react-pdf/renderer": "^3.4.5",
		"aws-amplify": "^6.0.28",
		"clsx": "^2.1.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-loader-spinner": "^6.1.6"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^6.6.2",
		"@testing-library/react": "^15.0.7",
		"@testing-library/user-event": "^14.5.2",
		"@types/react": "^18.3.12",
		"@types/react-dom": "^18.3.1",
		"@typescript-eslint/eslint-plugin": "^7.18.0",
		"@typescript-eslint/parser": "^7.18.0",
		"@vitejs/plugin-react": "^4.3.3",
		"concurrently": "^9.0.1",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-jest-dom": "^5.4.0",
		"eslint-plugin-jsx-a11y": "^6.10.1",
		"eslint-plugin-react": "^7.37.2",
		"eslint-plugin-react-hooks": "^4.6.2",
		"eslint-plugin-react-refresh": "^0.4.13",
		"happy-dom": "^14.12.3",
		"prettier": "^3.3.3",
		"stylelint": "^16.10.0",
		"stylelint-config-hudochenkov": "^10.0.0",
		"stylelint-config-standard": "^36.0.1",
		"typescript": "^5.6.3",
		"vite": "^5.4.10",
		"vitest": "^1.6.0"
	}
}
