{
	"name": "prompt-injection-api",
	"version": "1.0.3",
	"type": "module",
	"scripts": {
		"build": "tsc --noEmit",
		"dev": "cross-env NODE_ENV=development tsx watch -r dotenv/config src/server/index.ts",
		"start": "cross-env NODE_ENV=prodlite tsx -r dotenv/config src/server/index.ts",
		"codecheck": "concurrently \"prettier --check .\" \"eslint --max-warnings 0 .\"",
		"format": "prettier --write .",
		"lint": "eslint . --fix",
		"test": "jest"
	},
	"dependencies": {
		"cors": "^2.8.5",
		"d3-dsv": "^2.0.0",
		"dotenv": "^16.4.5",
		"express": "^4.21.1",
		"express-session": "^1.18.1",
		"langchain": "^0.1.33",
		"memorystore": "^1.6.7",
		"openai": "^4.33.0",
		"openai-chat-tokens": "^0.2.8",
		"pdf-parse": "^1.1.1",
		"query-types": "^0.1.4"
	},
	"devDependencies": {
		"@jest/globals": "^29.7.0",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/express-session": "^1.18.0",
		"@types/node": "^20.16.15",
		"@types/supertest": "^6.0.2",
		"@typescript-eslint/eslint-plugin": "^7.18.0",
		"@typescript-eslint/parser": "^7.18.0",
		"concurrently": "^9.0.1",
		"cross-env": "^7.0.3",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-jest": "^28.8.3",
		"jest-junit": "^16.0.0",
		"prettier": "^3.3.3",
		"supertest": "^7.0.0",
		"ts-jest": "^29.2.5",
		"tsx": "^4.19.1",
		"typescript": "^5.6.3"
	}
}
