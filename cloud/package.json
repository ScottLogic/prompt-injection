{
	"name": "cloud",
	"version": "0.1.0",
	"bin": {
		"cloud": "bin/cloud.js"
	},
	"scripts": {
		"cdk:synth": "cdk synth -q \"*\"",
		"cdk:deploy": "cdk deploy --app cdk.out",
		"cdk:deploy:all": "cdk deploy --app cdk.out --all",
		"cdk:destroy": "cdk destroy --app cdk.out",
		"cdk:destroy:all": "cdk destroy --app cdk.out --all",
		"build:edge": "tsc -p lib/lambdas/tsconfig.json",
		"codecheck": "concurrently \"npm run lint:check\" \"npm run format:check\"",
		"format": "prettier . --write",
		"format:check": "prettier . --check",
		"lint": "eslint . --ext .ts --fix",
		"lint:check": "eslint . --ext .ts"
	},
	"devDependencies": {
		"@types/aws-lambda": "^8.10.136",
		"@types/node": "^18.19.24",
		"@types/source-map-support": "^0.5.10",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"aws-cdk": "^2.132.1",
		"concurrently": "^8.2.2",
		"eslint": "^8.57.0",
		"prettier": "^3.2.5",
		"ts-node": "^10.9.2",
		"typescript": "^5.4.2"
	},
	"dependencies": {
		"@aws-cdk/aws-scheduler-alpha": "^2.132.1-alpha.0",
		"@aws-cdk/aws-scheduler-targets-alpha": "^2.132.1-alpha.0",
		"@aws-sdk/client-ecs": "^3.533.0",
		"@aws-sdk/client-secrets-manager": "^3.533.0",
		"aws-cdk-lib": "^2.132.1",
		"constructs": "^10.3.0",
		"dotenv": "^16.4.5",
		"source-map-support": "^0.5.21"
	}
}
